  CAPTION "SwpBuild Configs"
  EXSTYLE 0 // 0x00000000L
  STYLE DS_SYSMODAL | DS_FIXEDSYS | DS_SETFONT | DS_MODALFRAME | DS_CENTER | WS_DLGFRAME | WS_BORDER | WS_VISIBLE | WS_POPUP | NOT WS_SYSMENU // 0x90C008CAL
  FONT 10, "MS Shell Dlg", 0, 0, 1
BEGIN
  CONTROL "", 510, "static", SS_BITMAP | SS_SUNKEN | WS_VISIBLE | WS_CHILD, 5, 5, 60, 285
  CONTROL "Configs", -1, "button", BS_GROUPBOX | WS_VISIBLE | WS_CHILD, 40, 2, 175, 120
  CONTROL "  Fullscreen", 501, "button", BS_AUTOCHECKBOX | WS_TABSTOP | WS_VISIBLE | WS_CHILD, 50, 10, 130, 14
  CONTROL "Resolution:", -1, "static", SS_LEFT | WS_VISIBLE | WS_CHILD, 50, 30, 70, 14
  CONTROL "", 502, "combobox", CBS_DROPDOWNLIST | CBS_HASSTRINGS | WS_TABSTOP | WS_VSCROLL | WS_VISIBLE | WS_CHILD, 110, 30, 60, 60
  CONTROL "Color Depth:", -1, "static", SS_LEFT | WS_VISIBLE | WS_CHILD, 50, 50, 70, 14
  CONTROL "", 503, "combobox", CBS_DROPDOWNLIST | CBS_HASSTRINGS | WS_TABSTOP | WS_VSCROLL | WS_VISIBLE | WS_CHILD, 110, 50, 60, 45
  CONTROL "Refreshrate:", -1, "static", SS_LEFT | WS_VISIBLE | WS_CHILD, 50, 70, 70, 14
  CONTROL "", 504, "combobox", CBS_DROPDOWNLIST | CBS_HASSTRINGS | WS_TABSTOP | WS_VSCROLL | WS_VISIBLE | WS_CHILD, 110, 70, 60, 45
  CONTROL "Brightness:", -1, "static", SS_LEFT | WS_VISIBLE | WS_CHILD, 50, 90, 70, 14
  CONTROL "", 505, "combobox", CBS_DROPDOWNLIST | CBS_HASSTRINGS | WS_TABSTOP | WS_VSCROLL | WS_VISIBLE | WS_CHILD, 110, 90, 60, 75
  CONTROL "&Save and Exit SwpBuild", 522, "button", BS_PUSHBUTTON | WS_TABSTOP | WS_VISIBLE | WS_CHILD, 40, 125, 100, 12
  CONTROL "&Cancel", 523, "button", BS_DEFPUSHBUTTON | WS_TABSTOP | WS_VISIBLE | WS_CHILD, 165, 125, 50, 12
END
#endif

